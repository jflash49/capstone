
{#  src/Capstone/QuizBundle/Resources/views/report.html.twig#}
{% extends '::admin/layout.html.twig' %}
{% block title %} FunQ Quiz{% endblock %}
{% block subheader %}

{% endblock %}
{% block body %} 
<div class="row">
<div class="large-12 columns"">
<h1> {{ name }} </h1> 
<table class="records_list">
        <thead>
            <tr>
                <th>No. </th>
                <th>First Name</th>
                <th>Last Name</th>
                <th>Gender </th>
                <th> School </th>
                <th>IQ</th>
        </thead>
        <tbody>
        {% for reg in school %}
            <tr>
                {#<td><a href="{{ path('classification_show', { 'id': entity.classificationIq }) }}">{{ entity.classificationIq }}</a></td>
                reg.schoolId #}<td>{{loop.index}}</td>
                <td>{{reg.firstName}}</td>
                <td>{{reg.lastName}}</td>
                <td>{{reg.gender}}</td>
                <td>{{reg.school}}</td>
                <td>{{reg.iQ}}</td>
        
                
            </tr>
        {% endfor %}
        </tbody>
    </table>
 </div>
 </div>
{% endblock %}